inline void lambdaValues() {
#if SPECTRUM_SAMPLES == 3
	s[0] = 0.645; s[1] = 0.526; s[2] = 0.444;	
#else
	// wavelengths in m_wavelengths are in nano-meters
	// conversion to micro-meters
		for (int i=0; i<SPECTRUM_SAMPLES; i++)
			s[i] = 0.001 * (0.5 * (m_wavelengths[i] + m_wavelengths[i+1]));
#endif
}

inline void lambdaValuesPower(Float p) {
#if SPECTRUM_SAMPLES == 3
	s[0] = std::pow(0.645, p); s[1] = std::pow(0.526, p); s[2] = std::pow(0.444, p);	
#else
	// wavelengths in m_wavelengths are in nano-meters
	// conversion to micro-meters
		for (int i=0; i<SPECTRUM_SAMPLES; i++)
			s[i] = std::pow(0.001 * (0.5 * (m_wavelengths[i] + m_wavelengths[i+1])), p);
#endif
}

